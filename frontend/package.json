	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint . --ext .jsx",
		"lint:fix": "eslint . --fix --ext .jsx",
		"prettier": "prettier --check **/*.jsx",
		"prettier:fix": "prettier --write **/*.jsx"
	},
	"eslintConfig": {
		"rules": {
			"indent": [
				"error",4
			]
		},
		"parserOptions": {
			"sourceType": "module",
			"ecmaVersion": 2018,
			"ecmaFeatures": {
				"jsx": true
			}
		},
		"extends": [
			"prettier"
		],
		"ignorePatterns": [
			"node_modules/"
		],
		"plugins": [
			"prettier",
			"react"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"semi": true,
		"bracketSpacing": false,
		"singleQuote": true,
		"bracketSameLine": true
	}
}
